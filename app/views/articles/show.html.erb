<h1> <%= @article.title %> </h1> <br/>
<p> <%= @article.body %> </p> <br/>
<h3> <%= link_to "Go Back", root_path %> </h3>
<ul>
  <li><%= link_to "Edit", edit_article_path(@article) %></li>
  <li><%= link_to "Destroy", article_path(@article), data: {
                    method: :delete,
                    confirm: "Are you sure?"
                  } %></li>
</ul>